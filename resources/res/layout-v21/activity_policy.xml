<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <ScrollView android:layout_width="wrap_content" android:layout_height="wrap_content">
        <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView android:layout_gravity="center" android:id="@+id/card_view0" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="2dp" android:elevation="3dp">
                <TextView android:textSize="16dp" android:textColor="@color/colorPrimary" android:id="@+id/heading" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="RTO VEHICLE OWNER DETAILS PRIVACY POLICY"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView android:layout_gravity="center" android:id="@+id/card_view1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="2dp" android:layout_below="@+id/card_view0" android:elevation="3dp">
                <TextView android:textSize="12dp" android:id="@+id/privacy1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacy1"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView android:layout_gravity="center" android:id="@+id/card_view2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="2dp" android:layout_below="@+id/card_view1" android:elevation="3dp">
                <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:textSize="16dp" android:textStyle="bold" android:id="@+id/privacyheading1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacyheading2"/>
                    <TextView android:textSize="12dp" android:id="@+id/privacy2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacy2" android:layout_below="@+id/privacyheading1"/>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView android:layout_gravity="center" android:id="@+id/card_view3" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="2dp" android:layout_below="@+id/card_view2" android:elevation="3dp">
                <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:textSize="16dp" android:textStyle="bold" android:id="@+id/privacyheading2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacyheading3"/>
                    <TextView android:textSize="12dp" android:id="@+id/privacy3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacy3" android:layout_below="@+id/privacyheading2"/>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView android:layout_gravity="center" android:id="@+id/card_view4" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="2dp" android:layout_below="@+id/card_view3" android:elevation="3dp">
                <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:textSize="16dp" android:textStyle="bold" android:id="@+id/privacyheading3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacyheading4"/>
                    <TextView android:textSize="12dp" android:id="@+id/privacy4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacy4" android:layout_below="@+id/privacyheading3"/>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView android:layout_gravity="center" android:id="@+id/card_view5" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="2dp" android:layout_below="@+id/card_view4" android:elevation="3dp">
                <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:textSize="16dp" android:textStyle="bold" android:id="@+id/privacyheading4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacyheading5"/>
                    <TextView android:textSize="12dp" android:id="@+id/privacy5" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="@string/privacy5" android:layout_below="@+id/privacyheading4"/>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
        </RelativeLayout>
    </ScrollView>
</LinearLayout>
